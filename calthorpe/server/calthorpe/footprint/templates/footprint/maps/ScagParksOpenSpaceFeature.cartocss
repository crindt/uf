.{{ htmlClass }}
{buffer-size: 50;}

[zoom=12] {
  line-color:#594;
  line-width:0.5;
  polygon-opacity:0.6;
  polygon-fill:#ae8;
}

[zoom=13] {
  line-color:#594;
  line-width:1;
  polygon-opacity:0.6;
  polygon-fill:#ae8;
}

[zoom>=14] {
  line-color:#594;
  line-width:1;
  polygon-opacity:0.6;
  polygon-fill:#ae8;
}
